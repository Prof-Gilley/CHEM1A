            <qtimetadatafield>
              <fieldlabel>assessment_question_identifierref</fieldlabel>
              <fieldentry>b2q7g8h9i0j1k2l3m4n5o6p7q</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html"><![CDATA[<div><p>In a coffee cup calorimeter, why is ΔH approximately equal to q<sub>p</sub>?</p></div>]]></mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="choice_a">
                <material>
                  <mattext texttype="text/plain"><![CDATA[Because volume is constant]]></mattext>
                </material>
              </response_label>
              <response_label ident="choice_b">
                <material>
                  <mattext texttype="text/plain"><![CDATA[Because pressure is constant and only PV work is done]]></mattext>
                </material>
              </response_label>
              <response_label ident="choice_c">
                <material>
                  <mattext texttype="text/plain"><![CDATA[Because no work is done]]></mattext>
                </material>
              </response_label>
              <response_label ident="choice_d">
                <material>
                  <mattext texttype="text/plain"><![CDATA[Because temperature is constant]]></mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">choice_b</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
      </item>

