      <!-- Question 2.1: Metal specific heat calorimetry -->
      <item ident="b2q1a2b3c4d5e6f7g8h9i0j1k" title="Determine metal specific heat">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>question_type</fieldlabel>
              <fieldentry>multiple_choice_question</fieldentry>
            </qtimetadatafield>
            <qtimetadatafield>
              <fieldlabel>points_possible</fieldlabel>
              <fieldentry>1.0</fieldentry>
            </qtimetadatafield>
            <qtimetadatafield>
              <fieldlabel>assessment_question_identifierref</fieldlabel>
              <fieldentry>b2q1a2b3c4d5e6f7g8h9i0j1k</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html"><![CDATA[<div><p>A 50.0 g piece of metal at 95.0°C is placed in 100.0 g of water at 22.0°C. The final temperature is 27.8°C. What is the specific heat of the metal? (c<sub>water</sub> = 4.184 J/g°C)</p></div>]]></mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="choice_a">
                <material>
                  <mattext texttype="text/plain"><![CDATA[0.449 J/g°C]]></mattext>
                </material>
              </response_label>
              <response_label ident="choice_b">
                <material>
                  <mattext texttype="text/plain"><![CDATA[0.835 J/g°C]]></mattext>
                </material>
              </response_label>
              <response_label ident="choice_c">
                <material>
                  <mattext texttype="text/plain"><![CDATA[0.296 J/g°C]]></mattext>
                </material>
              </response_label>
              <response_label ident="choice_d">
                <material>
                  <mattext texttype="text/plain"><![CDATA[1.23 J/g°C]]></mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">choice_c</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
      </item>

      <!-- Question 2.2: Heat of neutralization -->
      <item ident="b2q2b3c4d5e6f7g8h9i0j1k2l" title="Heat of neutralization">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>question_type</fieldlabel>
              <fieldentry>multiple_choice_question</fieldentry>
            </qtimetadatafield>
