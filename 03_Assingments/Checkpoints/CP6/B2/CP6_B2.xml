      <!-- Question 2.7: Coffee cup calorimeter ΔH -->
      <item ident="b2q7g8h9i0j1k2l3m4n5o6p7q" title="Coffee cup calorimeter principle">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>question_type</fieldlabel>
              <fieldentry>multiple_choice_question</fieldentry>
            </qtimetadatafield>
