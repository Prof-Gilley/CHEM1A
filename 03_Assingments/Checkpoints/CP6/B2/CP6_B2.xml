            <render_choice>
              <response_label ident="choice_a">
                <material>
                  <mattext texttype="text/plain"><![CDATA[-28.5 kJ/mol]]></mattext>
                </material>
              </response_label>
              <response_label ident="choice_b">
                <material>
                  <mattext texttype="text/plain"><![CDATA[-42.7 kJ/mol]]></mattext>
                </material>
              </response_label>
              <response_label ident="choice_c">
                <material>
                  <mattext texttype="text/plain"><![CDATA[-56.9 kJ/mol]]></mattext>
                </material>
              </response_label>
              <response_label ident="choice_d">
                <material>
                  <mattext texttype="text/plain"><![CDATA[-71.1 kJ/mol]]></mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">choice_c</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
      </item>

