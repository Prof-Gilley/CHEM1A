              </response_label>
              <response_label ident="choice_b">
                <material>
                  <mattext texttype="text/plain"><![CDATA[-200 kJ]]></mattext>
                </material>
              </response_label>
              <response_label ident="choice_c">
                <material>
                  <mattext texttype="text/plain"><![CDATA[+300 kJ]]></mattext>
                </material>
              </response_label>
              <response_label ident="choice_d">
                <material>
                  <mattext texttype="text/plain"><![CDATA[+200 kJ]]></mattext>
