      <!-- Question 5.1: Endothermic reaction characteristics -->
      <item ident="b5q1a2b3c4d5e6f7g8h9i0j1k" title="Endothermic reaction properties">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>question_type</fieldlabel>
              <fieldentry>multiple_choice_question</fieldentry>
            </qtimetadatafield>
            <qtimetadatafield>
              <fieldlabel>points_possible</fieldlabel>
              <fieldentry>1.0</fieldentry>
            </qtimetadatafield>
            <qtimetadatafield>
              <fieldlabel>assessment_question_identifierref</fieldlabel>
              <fieldentry>b5q1a2b3c4d5e6f7g8h9i0j1k</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html"><![CDATA[<div><p>Which statement about an endothermic reaction is correct?</p></div>]]></mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="choice_a">
                <material>
                  <mattext texttype="text/plain"><![CDATA[Heat flows from the system to the surroundings]]></mattext>
                </material>
              </response_label>
              <response_label ident="choice_b">
                <material>
                  <mattext texttype="text/plain"><![CDATA[The products have lower enthalpy than the reactants]]></mattext>
                </material>
              </response_label>
              <response_label ident="choice_c">
                <material>
                  <mattext texttype="text/plain"><![CDATA[ΔH is positive for the reaction]]></mattext>
                </material>
              </response_label>
              <response_label ident="choice_d">
                <material>
                  <mattext texttype="text/plain"><![CDATA[The reaction releases heat to the surroundings]]></mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">choice_c</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
      </item>

