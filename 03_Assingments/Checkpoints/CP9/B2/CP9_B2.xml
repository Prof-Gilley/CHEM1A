      <!-- Question 1 -->
      <item ident="q1_ground_state" title="[OpenStax 6.2 | COR 10.2 | Objective 1] Ground State Energy">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>question_type</fieldlabel>
              <fieldentry>multiple_choice_question</fieldentry>
            </qtimetadatafield>
            <qtimetadatafield>
              <fieldlabel>points_possible</fieldlabel>
              <fieldentry>1</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html"><![CDATA[<p>What is the energy of an electron in the ground state (n = 1) of a hydrogen atom?</p>
<p><strong>Constant:</strong> Bohr constant for hydrogen = -2.18 × 10⁻¹⁸ J</p>]]></mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="A">
                <material><mattext>+2.18 × 10⁻¹⁸ J</mattext></material>
              </response_label>
              <response_label ident="B">
                <material><mattext>-2.18 × 10⁻¹⁸ J</mattext></material>
              </response_label>
              <response_label ident="C">
                <material><mattext>-5.45 × 10⁻¹⁹ J</mattext></material>
              </response_label>
              <response_label ident="D">
                <material><mattext>0 J</mattext></material>
              </response_label>
              <response_label ident="E">
                <material><mattext>-1.09 × 10⁻¹⁸ J</mattext></material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="1" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">B</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">1</setvar>
            <displayfeedback feedbacktype="Response" linkrefid="correct_fb"/>
          </respcondition>
        </resprocessing>
        <itemfeedback ident="correct_fb">
          <flow_mat>
            <material>
              <mattext>E₁ = -2.18 × 10⁻¹⁸ J × (1/1²) = -2.18 × 10⁻¹⁸ J</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
      </item>

      <!-- Question 2 -->
      <item ident="q2_transition_energy" title="[OpenStax 6.2 | COR 10.3 | Objective 1] Transition Energy Calculation">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>question_type</fieldlabel>
              <fieldentry>multiple_choice_question</fieldentry>
            </qtimetadatafield>
            <qtimetadatafield>
              <fieldlabel>points_possible</fieldlabel>
              <fieldentry>1</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html"><![CDATA[<p>Calculate the energy change when an electron in hydrogen transitions from n = 3 to n = 2.</p>
<p><strong>Constant:</strong> Bohr constant for hydrogen = -2.18 × 10⁻¹⁸ J</p>]]></mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="A">
                <material><mattext>-3.03 × 10⁻¹⁹ J</mattext></material>
              </response_label>
              <response_label ident="B">
                <material><mattext>+3.03 × 10⁻¹⁹ J</mattext></material>
              </response_label>
              <response_label ident="C">
                <material><mattext>-1.82 × 10⁻¹⁸ J</mattext></material>
              </response_label>
              <response_label ident="D">
                <material><mattext>-6.06 × 10⁻¹⁹ J</mattext></material>
              </response_label>
              <response_label ident="E">
                <material><mattext>-1.51 × 10⁻¹⁹ J</mattext></material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="1" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">A</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">1</setvar>
            <displayfeedback feedbacktype="Response" linkrefid="correct_fb"/>
          </respcondition>
        </resprocessing>
        <itemfeedback ident="correct_fb">
          <flow_mat>
            <material>
              <mattext>ΔE = -2.18 × 10⁻¹⁸ × (1/4 - 1/9) = -2.18 × 10⁻¹⁸ × 0.139 = -3.03 × 10⁻¹⁹ J</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
      </item>

      <!-- Question 3 -->
      <item ident="q3_emission_absorption" title="[OpenStax 6.2 | COR 10.3 | Objective 2] Emission vs Absorption">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>question_type</fieldlabel>
              <fieldentry>multiple_choice_question</fieldentry>
            </qtimetadatafield>
            <qtimetadatafield>
              <fieldlabel>points_possible</fieldlabel>
              <fieldentry>1</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html"><![CDATA[<p>An electron in hydrogen moves from n = 2 to n = 4. This process:</p>]]></mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="A">
                <material><mattext>Emits a photon in the visible region</mattext></material>
              </response_label>
              <response_label ident="B">
                <material><mattext>Emits a photon in the UV region</mattext></material>
              </response_label>
              <response_label ident="C">
                <material><mattext>Absorbs a photon in the visible region</mattext></material>
              </response_label>
              <response_label ident="D">
                <material><mattext>Absorbs a photon in the IR region</mattext></material>
              </response_label>
              <response_label ident="E">
                <material><mattext>Requires no energy change</mattext></material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="1" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">C</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">1</setvar>
            <displayfeedback feedbacktype="Response" linkrefid="correct_fb"/>
          </respcondition>
        </resprocessing>
        <itemfeedback ident="correct_fb">
          <flow_mat>
            <material>
              <mattext>Upward transition = absorption; ΔE = +4.09 × 10⁻¹⁹ J; λ = 486 nm = visible</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
      </item>

      <!-- Question 4 -->
      <item ident="q4_wavelength_transition" title="[OpenStax 6.2 | COR 10.3 | Objective 1] Wavelength from Transition">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>question_type</fieldlabel>
