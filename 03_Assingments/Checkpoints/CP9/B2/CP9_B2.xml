        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html"><![CDATA[<p>What is the wavelength of light emitted when an electron in hydrogen falls from n = 3 to n = 2?</p>
<p><strong>Constant:</strong> Bohr constant for hydrogen = -2.18 × 10⁻¹⁸ J</p>]]></mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="A">
                <material><mattext>486 nm</mattext></material>
              </response_label>
              <response_label ident="B">
                <material><mattext>656 nm</mattext></material>
              </response_label>
              <response_label ident="C">
                <material><mattext>410 nm</mattext></material>
              </response_label>
              <response_label ident="D">
                <material><mattext>434 nm</mattext></material>
              </response_label>
              <response_label ident="E">
                <material><mattext>122 nm</mattext></material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="1" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">B</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">1</setvar>
            <displayfeedback feedbacktype="Response" linkrefid="correct_fb"/>
          </respcondition>
        </resprocessing>
        <itemfeedback ident="correct_fb">
          <flow_mat>
            <material>
              <mattext>ΔE = -3.03 × 10⁻¹⁹ J; λ = hc/|ΔE| = 656 nm - Hα line</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
      </item>

      <!-- Question 5 -->
      <item ident="q5_ionization_energy" title="[OpenStax 6.2 | COR 10.2 | Objective 1] Ionization Energy">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>question_type</fieldlabel>
              <fieldentry>multiple_choice_question</fieldentry>
            </qtimetadatafield>
            <qtimetadatafield>
              <fieldlabel>points_possible</fieldlabel>
              <fieldentry>1</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html"><![CDATA[<p>What is the minimum energy required to ionize a hydrogen atom in the ground state (remove electron completely)?</p>
<p><strong>Constant:</strong> Bohr constant for hydrogen = -2.18 × 10⁻¹⁸ J</p>]]></mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="A">
                <material><mattext>1.09 × 10⁻¹⁸ J</mattext></material>
              </response_label>
              <response_label ident="B">
                <material><mattext>2.18 × 10⁻¹⁸ J</mattext></material>
              </response_label>
              <response_label ident="C">
                <material><mattext>5.45 × 10⁻¹⁹ J</mattext></material>
              </response_label>
              <response_label ident="D">
                <material><mattext>4.36 × 10⁻¹⁸ J</mattext></material>
              </response_label>
              <response_label ident="E">
                <material><mattext>1.36 × 10⁻¹⁸ J</mattext></material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="1" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">B</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">1</setvar>
            <displayfeedback feedbacktype="Response" linkrefid="correct_fb"/>
          </respcondition>
        </resprocessing>
        <itemfeedback ident="correct_fb">
          <flow_mat>
            <material>
              <mattext>ΔE = E∞ - E₁ = 0 - (-2.18 × 10⁻¹⁸) = +2.18 × 10⁻¹⁸ J</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
      </item>

