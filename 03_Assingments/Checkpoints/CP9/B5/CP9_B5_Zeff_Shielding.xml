      <!-- Question 12 -->
      <item ident="q12_zeff_calculation" title="[OpenStax 6.5 | COR 11.2.1 | Objective 2] Z_eff Calculation Concept">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
