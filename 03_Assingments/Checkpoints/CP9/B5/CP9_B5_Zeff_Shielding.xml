          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html"><![CDATA[<p>As you move from left to right across a period, Z<sub>eff</sub> generally:</p>]]></mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="A">
                <material><mattext>Decreases</mattext></material>
              </response_label>
              <response_label ident="B">
                <material><mattext>Remains constant</mattext></material>
              </response_label>
              <response_label ident="C">
                <material><mattext>Increases</mattext></material>
              </response_label>
              <response_label ident="D">
                <material><mattext>Increases then decreases</mattext></material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
