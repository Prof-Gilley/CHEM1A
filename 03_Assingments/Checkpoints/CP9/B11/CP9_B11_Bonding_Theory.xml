          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="A">
                <material><mattext>C with single bonds to each O</mattext></material>
              </response_label>
              <response_label ident="B">
                <material><mattext>C with double bonds to each O</mattext></material>
              </response_label>
              <response_label ident="C">
                <material><mattext>C with triple bond to one O, single to other</mattext></material>
              </response_label>
              <response_label ident="D">
                <material><mattext>C with one double, one quadruple bond</mattext></material>
              </response_label>
            </render_choice>
          </response_lid>
