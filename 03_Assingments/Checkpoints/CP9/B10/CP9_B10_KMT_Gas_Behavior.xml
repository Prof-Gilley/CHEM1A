    <section ident="root_section">
      
      <!-- Question 1 -->
      <item ident="q1_kmt_volume" title="[OpenStax 9.5 | COR 8.7 | Objective 2] KMT Postulate - Volume">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>question_type</fieldlabel>
              <fieldentry>multiple_choice_question</fieldentry>
            </qtimetadatafield>
            <qtimetadatafield>
              <fieldlabel>points_possible</fieldlabel>
              <fieldentry>1</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html"><![CDATA[<p>According to KMT, gas particles:</p>]]></mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="A">
                <material><mattext>Occupy most of the container volume</mattext></material>
              </response_label>
              <response_label ident="B">
                <material><mattext>Have negligible volume compared to container</mattext></material>
              </response_label>
              <response_label ident="C">
                <material><mattext>Are tightly packed like liquids</mattext></material>
              </response_label>
              <response_label ident="D">
                <material><mattext>Occupy exactly half the container volume</mattext></material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="1" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">B</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">1</setvar>
            <displayfeedback feedbacktype="Response" linkrefid="correct_fb"/>
          </respcondition>
        </resprocessing>
        <itemfeedback ident="correct_fb">
          <flow_mat>
            <material>
              <mattext>Gas particles are point masses; container is mostly empty space</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
      </item>

      <!-- Question 2 -->
      <item ident="q2_kmt_motion" title="[OpenStax 9.5 | COR 8.7 | Objective 2] KMT Postulate - Motion">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>question_type</fieldlabel>
              <fieldentry>multiple_choice_question</fieldentry>
            </qtimetadatafield>
            <qtimetadatafield>
              <fieldlabel>points_possible</fieldlabel>
              <fieldentry>1</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html"><![CDATA[<p>According to KMT, gas particles move:</p>]]></mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="A">
                <material><mattext>In circular orbits</mattext></material>
              </response_label>
              <response_label ident="B">
                <material><mattext>In straight lines until collision</mattext></material>
              </response_label>
              <response_label ident="C">
                <material><mattext>Very slowly at room temperature</mattext></material>
              </response_label>
              <response_label ident="D">
                <material><mattext>Only when heated</mattext></material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="1" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">B</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">1</setvar>
            <displayfeedback feedbacktype="Response" linkrefid="correct_fb"/>
          </respcondition>
        </resprocessing>
        <itemfeedback ident="correct_fb">
          <flow_mat>
            <material>
              <mattext>Constant, random, straight-line motion between collisions</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
      </item>

      <!-- Question 3 -->
      <item ident="q3_kmt_collisions" title="[OpenStax 9.5 | COR 8.7 | Objective 2] KMT Postulate - Collisions">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>question_type</fieldlabel>
              <fieldentry>multiple_choice_question</fieldentry>
            </qtimetadatafield>
            <qtimetadatafield>
              <fieldlabel>points_possible</fieldlabel>
              <fieldentry>1</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html"><![CDATA[<p>In KMT, collisions between gas particles are:</p>]]></mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="A">
                <material><mattext>Inelastic (energy is lost)</mattext></material>
              </response_label>
              <response_label ident="B">
                <material><mattext>Perfectly elastic (no energy lost)</mattext></material>
              </response_label>
              <response_label ident="C">
                <material><mattext>Rare events</mattext></material>
              </response_label>
              <response_label ident="D">
                <material><mattext>Impossible</mattext></material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="1" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">B</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">1</setvar>
            <displayfeedback feedbacktype="Response" linkrefid="correct_fb"/>
          </respcondition>
        </resprocessing>
        <itemfeedback ident="correct_fb">
          <flow_mat>
            <material>
              <mattext>Elastic collisions: KE_before = KE_after</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
      </item>

      <!-- Question 4 -->
