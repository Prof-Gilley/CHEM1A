                <material><mattext>Pressure only</mattext></material>
              </response_label>
              <response_label ident="D">
                <material><mattext>Number of collisions</mattext></material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="1" minvalue="0" varname="SCORE" vartype="Decimal"/>
