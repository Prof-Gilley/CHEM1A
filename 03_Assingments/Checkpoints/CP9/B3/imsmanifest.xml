      <!-- Question 1 -->
      <item ident="q1_debroglie" title="[OpenStax 6.3 | COR 10.4 | Objective 1] de Broglie Wavelength">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>question_type</fieldlabel>
              <fieldentry>multiple_choice_question</fieldentry>
            </qtimetadatafield>
            <qtimetadatafield>
              <fieldlabel>points_possible</fieldlabel>
              <fieldentry>1</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html"><![CDATA[<p>Calculate the de Broglie wavelength of an electron moving at 1.0 × 10⁷ m/s.</p>
<p><strong>Constants:</strong> h = 6.626 × 10⁻³⁴ J·s, m<sub>e</sub> = 9.11 × 10⁻³¹ kg</p>]]></mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="A">
                <material><mattext>7.3 × 10⁻¹¹ m</mattext></material>
              </response_label>
              <response_label ident="B">
                <material><mattext>7.3 × 10⁻¹⁰ m</mattext></material>
              </response_label>
              <response_label ident="C">
                <material><mattext>7.3 × 10⁻⁹ m</mattext></material>
              </response_label>
              <response_label ident="D">
                <material><mattext>7.3 × 10⁻¹² m</mattext></material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="1" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">A</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">1</setvar>
            <displayfeedback feedbacktype="Response" linkrefid="correct_fb"/>
          </respcondition>
        </resprocessing>
        <itemfeedback ident="correct_fb">
          <flow_mat>
            <material>
              <mattext>λ = h/mv = 6.626 × 10⁻³⁴ / (9.11 × 10⁻³¹ × 1.0 × 10⁷) = 7.3 × 10⁻¹¹ m</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
      </item>

      <!-- Question 2 -->
      <item ident="q2_heisenberg" title="[OpenStax 6.3 | COR 10.4 | Objective 2] Heisenberg Uncertainty Principle">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>question_type</fieldlabel>
              <fieldentry>multiple_choice_question</fieldentry>
            </qtimetadatafield>
            <qtimetadatafield>
              <fieldlabel>points_possible</fieldlabel>
              <fieldentry>1</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html"><![CDATA[<p>The Heisenberg uncertainty principle states that:</p>]]></mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="A">
                <material><mattext>Electrons have both wave and particle properties</mattext></material>
              </response_label>
              <response_label ident="B">
                <material><mattext>Energy is quantized in atoms</mattext></material>
              </response_label>
              <response_label ident="C">
                <material><mattext>Position and momentum cannot both be known precisely</mattext></material>
              </response_label>
              <response_label ident="D">
                <material><mattext>Electrons orbit in defined circular paths</mattext></material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="1" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">C</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">1</setvar>
            <displayfeedback feedbacktype="Response" linkrefid="correct_fb"/>
          </respcondition>
        </resprocessing>
        <itemfeedback ident="correct_fb">
          <flow_mat>
            <material>
              <mattext>Δx × Δp ≥ ℏ/2 - fundamental limitation on simultaneous measurement</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
      </item>

