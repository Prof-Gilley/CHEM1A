            <mattext texttype="text/html"><![CDATA[<p>The Aufbau principle states that:</p>]]></mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="A">
                <material><mattext>Electrons fill lowest energy orbitals first</mattext></material>
