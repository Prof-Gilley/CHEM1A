        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html"><![CDATA[Rank the following molecules by <b>increasing polarity</b> of their central bond:]]></mattext>
          </material>
          <response_lid ident="response2" rcardinality="single">
            <render_choice>
              <response_label ident="a">
                <material>
                  <mattext texttype="text/html"><![CDATA[CF<sub>4</sub> < CHF<sub>3</sub> < CH<sub>2</sub>F<sub>2</sub> < CH<sub>3</sub>F]]></mattext>
                </material>
              </response_label>
              <response_label ident="b">
                <material>
                  <mattext texttype="text/html"><![CDATA[CH<sub>3</sub>F < CH<sub>2</sub>F<sub>2</sub> < CHF<sub>3</sub> < CF<sub>4</sub>]]></mattext>
                </material>
              </response_label>
              <response_label ident="c">
                <material>
                  <mattext texttype="text/html"><![CDATA[CF<sub>4</sub> = CHF<sub>3</sub> = CH<sub>2</sub>F<sub>2</sub> = CH<sub>3</sub>F]]></mattext>
                </material>
              </response_label>
              <response_label ident="d">
                <material>
                  <mattext texttype="text/html"><![CDATA[CH<sub>2</sub>F<sub>2</sub> < CH<sub>3</sub>F < CHF<sub>3</sub> < CF<sub>4</sub>]]></mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response2">c</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
      </item>

